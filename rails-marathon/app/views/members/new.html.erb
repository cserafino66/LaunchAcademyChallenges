<h1>New House <%= @house.name %> Member Form</h1>


<%= form_for([@house, @member]) do |f| %>
  <% if @member.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@member.errors.count, "error") %> prohibited this event from being saved:</h2>

      <ul>
      <% @member.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="">
    <%= f.label :first_name, 'First Name' %>
    <%= f.text_field :first_name %>
  </div>

  <div class="">
    <%= f.label :last_name, 'Last Name' %>
    <%= f.text_field :last_name %>
  </div>
  <div class="">
    <%= f.submit 'Add Member' %>
  </div>
<% end %>
